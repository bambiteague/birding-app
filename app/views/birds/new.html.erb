<%= form_with(model: [:bird], method: :post, local: true)  do |f| %>
  <%= f.label :species %>
  <%= f.text_field :title %>
    <br>
    <br>
  <%= f.label :location %>
  <%= f.text_field :location
    <br>
    <br>
  <%=  f.label :visual_description%>
  <%=  f.text_area :visual_description %>
    <br>
    <br>
  <%= f.label :call_description %>
  <%= f.text_area :call_description %>
  <br>
  <br>
  <%=  f.label :quantity %>
  <%=  f.text_field :quantity %>
    <br>
    <br>
  <%= f.submit %>
<% end %>
